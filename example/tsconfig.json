{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "baseUrl": [
        "."
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "metro.config.mjs",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
